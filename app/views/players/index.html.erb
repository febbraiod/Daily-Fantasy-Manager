  <table>
    <tr>
      <th align="left">Name</th>
      <th align="left">Position</th>
      <th align="left">Ave Points</th> 
      <th align="left">Salary</th>
      <th align="left">CPP</th>
      <th align="left">Ceiling</th>
      <th align="left">Floor</th>
    </tr>
    <tr>
      <td class='benchmark'>Ave QB</td>
      <td class='benchmark'>QB</td>
      <td class='benchmark'><%='%.2f' % (@ave_qb[:points]) %></td> 
      <td class='benchmark'><%=@ave_qb[:salary]%></td>
      <td class='benchmark'><%='%.2f' % (@ave_qb[:value])%></td>
      <td class='benchmark'><%='%.2f' % @ave_qb[:ceiling]%></td>
      <td class='benchmark'><%='%.2f' % @ave_qb[:floor]%></td>
    </tr>
    <% @qbs.each do |p| %>
      <tr>
        <td><%=p.name%></td>
        <td><%=p.position%></td>
        <td><%='%.2f' % (Player.average_points(p.projected_points)) unless p.projected_points.length == 0 %></td> 
        <td><%=p.salary%></td>
        <td><%='%.2f' % (p.player_value)%></td>
        <td><%=p.projected_points.max%></td>
        <td><%=p.projected_points.min%></td>
      </tr>
    <% end %>
  </table>

