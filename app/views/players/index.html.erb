  <table>
    <tr>
      <th align="left">Name</th>
      <th align="left">Position</th>
      <th align="left">Ave Points</th> 
      <th align="left">Salary</th>
      <th align="left">CPP</th>
      <th align="left">Ceiling</th>
      <th align="left">Floor</th>
    </tr>
    <% @qbs.each do |p| %>
      <tr>
        <td><%=p.name%></td>
        <td><%=p.position%></td>
        <td><%='%.2f' % (p.projected_points.reduce(0, :+)/p.projected_points.length) unless p.projected_points.length == 0 %></td> 
        <td><%=p.salary%></td>
        <td><%='%.2f' % (p.player_value)%></td>
        <td><%=p.projected_points.max%></td>
        <td><%=p.projected_points.min%></td>
      </tr>
    <% end %>
  </table>

